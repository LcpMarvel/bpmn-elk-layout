  <bpmn:process id="process_event_gateway_parallel" name="并行事件网关测试" isExecutable="true">
    <bpmn:startEvent id="start_1" name="开始">
      <bpmn:outgoing>flow_start</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="task_init" name="初始化订单">
      <bpmn:incoming>flow_start</bpmn:incoming>
      <bpmn:outgoing>flow_to_parallel_gateway</bpmn:outgoing>
    </bpmn:task>
    <bpmn:eventBasedGateway id="gateway_event_parallel" name="并行事件网关" gatewayDirection="Diverging" eventGatewayType="Parallel">
      <bpmn:incoming>flow_to_parallel_gateway</bpmn:incoming>
      <bpmn:outgoing>flow_parallel_order</bpmn:outgoing>
      <bpmn:outgoing>flow_parallel_payment</bpmn:outgoing>
      <bpmn:outgoing>flow_parallel_timer</bpmn:outgoing>
    </bpmn:eventBasedGateway>
    <bpmn:intermediateCatchEvent id="catch_message_order" name="等待订单确认">
      <bpmn:incoming>flow_parallel_order</bpmn:incoming>
      <bpmn:outgoing>flow_order_process</bpmn:outgoing>
      <bpmn:messageEventDefinition messageRef="msg_order" />
    </bpmn:intermediateCatchEvent>
    <bpmn:intermediateCatchEvent id="catch_message_payment" name="等待支付确认">
      <bpmn:incoming>flow_parallel_payment</bpmn:incoming>
      <bpmn:outgoing>flow_payment_process</bpmn:outgoing>
      <bpmn:messageEventDefinition messageRef="msg_payment" />
    </bpmn:intermediateCatchEvent>
    <bpmn:intermediateCatchEvent id="catch_timer" name="等待超时">
      <bpmn:incoming>flow_parallel_timer</bpmn:incoming>
      <bpmn:outgoing>flow_timer_process</bpmn:outgoing>
      <bpmn:timerEventDefinition>
        <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">PT30M</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>
    <bpmn:task id="task_process_order" name="处理订单">
      <bpmn:incoming>flow_order_process</bpmn:incoming>
      <bpmn:outgoing>flow_order_to_join</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="task_process_payment" name="处理支付">
      <bpmn:incoming>flow_payment_process</bpmn:incoming>
      <bpmn:outgoing>flow_payment_to_join</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="task_timeout_handler" name="超时处理">
      <bpmn:incoming>flow_timer_process</bpmn:incoming>
      <bpmn:outgoing>flow_timeout_end</bpmn:outgoing>
    </bpmn:task>
    <bpmn:parallelGateway id="gateway_parallel_join" name="并行汇合" gatewayDirection="Converging">
      <bpmn:incoming>flow_order_to_join</bpmn:incoming>
      <bpmn:incoming>flow_payment_to_join</bpmn:incoming>
      <bpmn:outgoing>flow_join_to_exclusive</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:task id="task_complete" name="完成订单">
      <bpmn:incoming>flow_continue_complete</bpmn:incoming>
      <bpmn:outgoing>flow_complete_end</bpmn:outgoing>
    </bpmn:task>
    <bpmn:endEvent id="end_success" name="成功结束">
      <bpmn:incoming>flow_complete_end</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="end_timeout" name="超时结束">
      <bpmn:incoming>flow_timeout_end</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:eventBasedGateway id="gateway_event_exclusive" name="排他事件网关" gatewayDirection="Diverging">
