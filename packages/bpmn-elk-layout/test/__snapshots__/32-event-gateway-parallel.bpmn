      <bpmn:outgoing>flow_exclusive_cancel</bpmn:outgoing>
      <bpmn:outgoing>flow_exclusive_continue</bpmn:outgoing>
    </bpmn:eventBasedGateway>
    <bpmn:intermediateCatchEvent id="catch_signal_cancel" name="等待取消">
      <bpmn:incoming>flow_exclusive_cancel</bpmn:incoming>
      <bpmn:outgoing>flow_cancel_task</bpmn:outgoing>
      <bpmn:signalEventDefinition signalRef="sig_cancel" />
    </bpmn:intermediateCatchEvent>
    <bpmn:intermediateCatchEvent id="catch_timer_continue" name="等待继续">
      <bpmn:incoming>flow_exclusive_continue</bpmn:incoming>
      <bpmn:outgoing>flow_continue_complete</bpmn:outgoing>
      <bpmn:timerEventDefinition>
        <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">PT10M</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>
    <bpmn:task id="task_cancel" name="取消订单">
      <bpmn:incoming>flow_cancel_task</bpmn:incoming>
      <bpmn:outgoing>flow_cancel_end</bpmn:outgoing>
    </bpmn:task>
    <bpmn:endEvent id="end_cancelled" name="已取消">
      <bpmn:incoming>flow_cancel_end</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:eventBasedGateway id="gateway_instantiate" name="实例化事件网关" gatewayDirection="Diverging" instantiate="true" />
