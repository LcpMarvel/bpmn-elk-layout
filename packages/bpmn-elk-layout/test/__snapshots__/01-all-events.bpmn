  <bpmn:escalation id="esc_1" name="测试升级" escalationCode="ESC_001" />
  <bpmn:process id="process_all_events" name="所有事件类型测试" isExecutable="true">
    <bpmn:startEvent id="start_none" name="空启动">
      <bpmn:outgoing>flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:startEvent id="start_message" name="消息启动">
      <bpmn:messageEventDefinition messageRef="msg_1" />
    </bpmn:startEvent>
    <bpmn:startEvent id="start_timer" name="定时启动">
      <bpmn:timerEventDefinition>
        <bpmn:timeCycle xsi:type="bpmn:tFormalExpression">R/PT1H</bpmn:timeCycle>
      </bpmn:timerEventDefinition>
    </bpmn:startEvent>
    <bpmn:startEvent id="start_signal" name="信号启动">
      <bpmn:signalEventDefinition signalRef="sig_1" />
    </bpmn:startEvent>
    <bpmn:startEvent id="start_conditional" name="条件启动">
      <bpmn:conditionalEventDefinition>
        <bpmn:condition xsi:type="bpmn:tFormalExpression">${amount &gt; 1000}</bpmn:condition>
      </bpmn:conditionalEventDefinition>
    </bpmn:startEvent>
    <bpmn:startEvent id="start_multiple" name="多重启动" />
    <bpmn:startEvent id="start_parallel_multiple" name="并行多重启动" />
    <bpmn:intermediateCatchEvent id="catch_message" name="接收消息">
      <bpmn:incoming>flow_1</bpmn:incoming>
      <bpmn:outgoing>flow_2</bpmn:outgoing>
      <bpmn:messageEventDefinition messageRef="msg_1" />
    </bpmn:intermediateCatchEvent>
    <bpmn:intermediateCatchEvent id="catch_timer" name="等待定时">
      <bpmn:timerEventDefinition>
        <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">PT30M</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>
    <bpmn:intermediateCatchEvent id="catch_signal" name="接收信号">
      <bpmn:signalEventDefinition signalRef="sig_1" />
    </bpmn:intermediateCatchEvent>
    <bpmn:intermediateCatchEvent id="catch_conditional" name="等待条件">
      <bpmn:conditionalEventDefinition>
        <bpmn:condition xsi:type="bpmn:tFormalExpression">${status == 'ready'}</bpmn:condition>
      </bpmn:conditionalEventDefinition>
    </bpmn:intermediateCatchEvent>
    <bpmn:intermediateCatchEvent id="catch_link" name="链接接收">
      <bpmn:linkEventDefinition name="link_point_A" />
    </bpmn:intermediateCatchEvent>
    <bpmn:intermediateThrowEvent id="throw_none" name="空抛出" />
    <bpmn:intermediateThrowEvent id="throw_message" name="发送消息">
      <bpmn:incoming>flow_2</bpmn:incoming>
      <bpmn:outgoing>flow_3</bpmn:outgoing>
      <bpmn:messageEventDefinition messageRef="msg_1" />
    </bpmn:intermediateThrowEvent>
    <bpmn:intermediateThrowEvent id="throw_signal" name="发送信号">
      <bpmn:signalEventDefinition signalRef="sig_1" />
    </bpmn:intermediateThrowEvent>
    <bpmn:intermediateThrowEvent id="throw_escalation" name="升级">
      <bpmn:escalationEventDefinition escalationRef="esc_1" />
    </bpmn:intermediateThrowEvent>
    <bpmn:intermediateThrowEvent id="throw_compensation" name="触发补偿">
      <bpmn:compensateEventDefinition />
    </bpmn:intermediateThrowEvent>
    <bpmn:intermediateThrowEvent id="throw_link" name="链接抛出">
      <bpmn:linkEventDefinition name="link_point_A" />
    </bpmn:intermediateThrowEvent>
    <bpmn:endEvent id="end_none" name="正常结束">
      <bpmn:incoming>flow_3</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="end_message" name="消息结束">
      <bpmn:messageEventDefinition messageRef="msg_1" />
    </bpmn:endEvent>
    <bpmn:endEvent id="end_error" name="错误结束">
      <bpmn:errorEventDefinition errorRef="err_1" />
    </bpmn:endEvent>
    <bpmn:endEvent id="end_escalation" name="升级结束">
      <bpmn:escalationEventDefinition escalationRef="esc_1" />
    </bpmn:endEvent>
    <bpmn:endEvent id="end_cancel" name="取消结束">
      <bpmn:cancelEventDefinition />
    </bpmn:endEvent>
    <bpmn:endEvent id="end_compensation" name="补偿结束">
      <bpmn:compensateEventDefinition />
    </bpmn:endEvent>
    <bpmn:endEvent id="end_signal" name="信号结束">
      <bpmn:signalEventDefinition signalRef="sig_1" />
    </bpmn:endEvent>
    <bpmn:endEvent id="end_terminate" name="终止结束">
      <bpmn:terminateEventDefinition />
    </bpmn:endEvent>
    <bpmn:endEvent id="end_multiple" name="多重结束" />
    <bpmn:sequenceFlow id="flow_1" sourceRef="start_none" targetRef="catch_message" />
    <bpmn:sequenceFlow id="flow_2" sourceRef="catch_message" targetRef="throw_message" />
    <bpmn:sequenceFlow id="flow_3" sourceRef="throw_message" targetRef="end_none" />
  </bpmn:process>
