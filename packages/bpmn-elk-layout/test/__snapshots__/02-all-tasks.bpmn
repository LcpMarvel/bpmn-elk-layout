      <bpmn:outgoing>flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="task_generic" name="通用任务">
      <bpmn:incoming>flow_1</bpmn:incoming>
      <bpmn:outgoing>flow_2</bpmn:outgoing>
    </bpmn:task>
    <bpmn:userTask id="task_user" name="用户任务">
      <bpmn:incoming>flow_2</bpmn:incoming>
      <bpmn:outgoing>flow_3</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="task_service" name="服务任务" implementation="${orderService.process()}">
      <bpmn:incoming>flow_3</bpmn:incoming>
      <bpmn:outgoing>flow_4</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:scriptTask id="task_script" name="脚本任务" scriptFormat="javascript">
      <bpmn:incoming>flow_4</bpmn:incoming>
      <bpmn:outgoing>flow_5</bpmn:outgoing>
      <bpmn:script>var result = input * 2; execution.setVariable('output', result);</bpmn:script>
    </bpmn:scriptTask>
    <bpmn:businessRuleTask id="task_business_rule" name="业务规则任务" implementation="dmn:decision_pricing">
      <bpmn:incoming>flow_5</bpmn:incoming>
      <bpmn:outgoing>flow_6</bpmn:outgoing>
    </bpmn:businessRuleTask>
    <bpmn:sendTask id="task_send" name="发送任务" implementation="${emailService.send()}" messageRef="msg_task">
      <bpmn:incoming>flow_6</bpmn:incoming>
      <bpmn:outgoing>flow_7</bpmn:outgoing>
    </bpmn:sendTask>
    <bpmn:receiveTask id="task_receive" name="接收任务" messageRef="msg_task">
      <bpmn:incoming>flow_7</bpmn:incoming>
      <bpmn:outgoing>flow_8</bpmn:outgoing>
    </bpmn:receiveTask>
    <bpmn:manualTask id="task_manual" name="手动任务">
      <bpmn:incoming>flow_8</bpmn:incoming>
      <bpmn:outgoing>flow_9</bpmn:outgoing>
    </bpmn:manualTask>
    <bpmn:endEvent id="end_1" name="结束">
      <bpmn:incoming>flow_9</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="flow_1" sourceRef="start_1" targetRef="task_generic" />
    <bpmn:sequenceFlow id="flow_2" sourceRef="task_generic" targetRef="task_user" />
    <bpmn:sequenceFlow id="flow_3" sourceRef="task_user" targetRef="task_service" />
    <bpmn:sequenceFlow id="flow_4" sourceRef="task_service" targetRef="task_script" />
    <bpmn:sequenceFlow id="flow_5" sourceRef="task_script" targetRef="task_business_rule" />
    <bpmn:sequenceFlow id="flow_6" sourceRef="task_business_rule" targetRef="task_send" />
    <bpmn:sequenceFlow id="flow_7" sourceRef="task_send" targetRef="task_receive" />
    <bpmn:sequenceFlow id="flow_8" sourceRef="task_receive" targetRef="task_manual" />
    <bpmn:sequenceFlow id="flow_9" sourceRef="task_manual" targetRef="end_1" />
