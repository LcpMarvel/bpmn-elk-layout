      <bpmndi:BPMNShape id="task_create_work_order_di" bpmnElement="task_create_work_order">
        <dc:Bounds x="421" y="60" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="task_associate_info_di" bpmnElement="task_associate_info">
