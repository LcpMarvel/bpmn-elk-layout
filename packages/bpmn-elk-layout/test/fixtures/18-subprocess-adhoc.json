            {
              "id": "adhoc_task_2",
              "width": 100, "height": 80,
              "bpmn": { "type": "task", "name": "可选任务2" },
              "labels": [{ "text": "可选任务2" }]
            },
            {
              "id": "adhoc_task_3",
              "width": 100, "height": 80,
              "bpmn": { "type": "task", "name": "可选任务3" },
              "labels": [{ "text": "可选任务3" }]
            }
          ]
        },
        {
          "id": "end_main",
          "width": 36, "height": 36,
          "bpmn": {
            "type": "endEvent",
            "eventDefinitionType": "none",
            "name": "结束"
          },
          "labels": [{ "text": "结束" }]
